    "redux:build:ssr": "ng build frontend-redux --prod && ng run frontend-redux:server:production",
    "redux:serve:ssr": "node dist/apps/frontend/redux/server/main.js",
    "testing:serve": "ng serve frontend-testing",
    "testing:serve:ssr:dev": "ng run frontend-testing:serve-ssr",
    "testing:build:ssr": "ng build frontend-testing --prod && ng run frontend-testing:server:production",
    "testing:serve:ssr": "node dist/apps/frontend/testing/server/main.js",
